{
  "name": "pkerrigan/xray",
  "description": "A basic PHP instrumentation library for AWS X-Ray",
  "keywords": [
    "AWS",
    "X-Ray",
    "AWS X-Ray",
    "APM",
    "Tracing"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Patrick Kerrigan",
      "homepage": "https://patrickkerrigan.uk/"
    },
    {
      "name": "Mauro Artizzu",
      "email": "mauro@fido.id"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-sockets": "*"
  },
  "autoload": {
    "psr-4": {
      "Pkerrigan\\Xray\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pkerrigan\\Xray\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0"
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
